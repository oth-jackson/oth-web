{
  "name": "oth-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3003",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check:type": "tsc -p tsconfig.json --noEmit",
    "generate:schema:auth": "npx @better-auth/cli generate --config ./better-auth.config.ts --output=./src/db/auth.schema.ts --y",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:push": "drizzle-kit push",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
